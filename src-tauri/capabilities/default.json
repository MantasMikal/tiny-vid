{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "log:default",
    "core:default",
    "core:window:allow-start-dragging",
    "opener:default",
    "os:default",
    "dialog:default",
    "shell:allow-spawn",
    "allow-ffmpeg-transcode-to-temp",
    "allow-ffmpeg-preview",
    "allow-preview-ffmpeg-command",
    "allow-ffmpeg-terminate",
    "allow-get-file-size",
    "allow-get-video-metadata",
    "allow-move-compressed-file",
    "allow-cleanup-temp-file",
    "allow-get-build-variant",
    "allow-get-pending-opened-files"
  ]
}
